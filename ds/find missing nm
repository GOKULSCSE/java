import java.util.*;
class numbers 
{
    void value(int[]arr){
        int n = arr.length+1;
        int sum =n*(n+1)/2;
           for(int i=0;i<arr.length;i++){
               sum -= arr[i];
           }
            System.out.print(sum);
        
    }  
    public static void main(String[] args) {
        int[] arr={1,4,3,2,6,8};
        numbers v1 = new numbers();
        v1.value(arr);
    }
}
